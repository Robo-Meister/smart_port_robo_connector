{
  "type": "domain_model",
  "model": "GateVisit",
  "description": "A single gate interaction spanning arrival, validation, clearance, and exit.",
  "requiredFields": [
    "id",
    "gateId",
    "direction",
    "status",
    "createdAt",
    "updatedAt"
  ],
  "fields": {
    "id": {"type": "string", "description": "Unique gate visit identifier (UUID)."},
    "gateId": {"type": "string", "description": "Gate reference identifier."},
    "direction": {"type": "string", "description": "inbound or outbound."},
    "status": {"type": "string", "description": "scheduled, checked_in, validating, held, cleared, exited, cancelled, no_show."},
    "slotAppointmentId": {"type": "string", "description": "Optional slot appointment reference."},
    "assetReference": {"type": "object", "description": "Asset being processed (container, pallet, trailer, batch, WIP lot)."},
    "carrierReference": {"type": "object", "description": "Carrier or transport reference (truck, rail, tugger, AGV)."},
    "operatorReference": {"type": "object", "description": "Driver or operator reference."},
    "checklistIds": {"type": "array", "items": "string", "description": "Associated checklist identifiers."},
    "holdIds": {"type": "array", "items": "string", "description": "Active hold identifiers."},
    "checkInAt": {"type": "string", "format": "date-time"},
    "clearedAt": {"type": "string", "format": "date-time"},
    "exitAt": {"type": "string", "format": "date-time"},
    "createdAt": {"type": "string", "format": "date-time"},
    "updatedAt": {"type": "string", "format": "date-time"}
  },
  "relationships": {
    "gate": {"model": "Gate", "field": "gateId"},
    "slotAppointment": {"model": "SlotAppointment", "field": "slotAppointmentId"}
  }
}
